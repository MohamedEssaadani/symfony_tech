{% extends "base.html.twig" %}

{% block body %}
	<div class="row">
		<div class="col-md-12">
			<div class="card">
				<div class="card-header d-block">
					<h3>Département Details</h3>
				</div>
				<div class="card-body">
					<div class="row">
						<label class="col-sm-6 col-lg-6 col-form-label">Département ID</label>
						<label class="col-sm-6 col-lg-6 col-form-label">{{departement.id}}</label>

						<label class="col-sm-6 col-lg-6 col-form-label">Nom Département</label>
						<label class="col-sm-6 col-lg-6 col-form-label">{{departement.nomDepartement}}</label>


					</div>
				</div>
			</div>
		</div>
	</div>

{% endblock %}
